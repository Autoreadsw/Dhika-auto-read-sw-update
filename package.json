{
  "name": "wajs",
  "version": "1.0.0",
  "description": "Script untuk membuat auto read status WhatsApp make wajs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js 2>> temp/error.log --max-old-space-size=8192",
    "pm2": "pm2 start index.js -e temp/error.log --name wajs -- --max-old-space-size=8192",
    "restart:pm2": "pm2 restart wajs",
    "stop:pm2": "pm2 stop wajs"
  },
